{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "exactOptionalPropertyTypes": false,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "module": "nodenext",
    "moduleDetection": "force",
    "noUncheckedIndexedAccess": true,
    "noUncheckedSideEffectImports": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    "types": [],
    "verbatimModuleSyntax": false,
  }
}
